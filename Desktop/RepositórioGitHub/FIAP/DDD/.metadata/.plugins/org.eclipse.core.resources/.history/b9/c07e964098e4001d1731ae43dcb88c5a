package br.com.fiap.strings;

public class Exemplo5Lenght {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		//Para obter o tamanho ou o numero de caracteres de uma string usa-se lenght
		
		
		
		String nomeCompleto = "Igor ferreira santana";
		
		
		System.out.println("A string "+(nomeCompleto)+" tem "+nomeCompleto.length()+"caracteres");
	}

}
